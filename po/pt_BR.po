#: ../main.c:20
#, c-format
msgid "Usage: %s [options]\n"
msgstr "Uso: %s [opções]\n"

#: ../main.c:21
msgid "Global options:\n"
msgstr "Opções globais:\n"

#: ../main.c:22
#, c-format
msgid "         -c <file>   specify config file (defaults to %s)\n"
msgstr ""
"         -c <arq.>   especifica arquivo de configuração (padrão : %s)\n"

#: ../main.c:23
msgid "         -D          do not differentiate destination IP addresses\n"
msgstr "         -D          não diferencia endereços IP de destino\n"

#: ../main.c:24
msgid "         -d          differentiate destination ports\n"
msgstr "         -d          diferencia portas de destino\n"

#: ../main.c:25
msgid "         -e          show end times\n"
msgstr "         -e          mostrar horario final\n"

#: ../main.c:26
#, c-format
msgid "         -f <file>   specify input file (defaults to %s)\n"
msgstr "         -f <arq.>   especifica arquivo de entrada  (padrão : %s)\n"

#: ../main.c:27
msgid "         -h          this help\n"
msgstr "         -h          mostra texto de ajuda\n"

#: ../main.c:28
msgid "         -L <file>   show time of first and last log entry in file\n"
msgstr ""
"         -L <arq.>   mostra horario inicial e final do arquivo de log\n"

#: ../main.c:29
msgid "         -l <time>   process recent events only (defaults to off)\n"
msgstr ""
"         -l <tempo>  processa eventos recentes somente (padrão : desligado)\n"

#: ../main.c:30
msgid "         -N          resolve service names\n"
msgstr "         -N          resolve nome de servico\n"

#: ../main.c:31
msgid "         -n          resolve host names\n"
msgstr "         -n          resolve nome de hosts\n"

#: ../main.c:32
msgid "         -P <format> use only parsers for specific formats\n"
msgstr "         -P <formato> utiliza somente os formatos especificados\n"

#: ../main.c:33
msgid "         -p          differentiate protocols\n"
msgstr "         -p          diferenciar protocolos\n"

#: ../main.c:34
msgid "         -S          do not differentiate source IP addresses\n"
msgstr "         -S          não diferencia endereços IP de origem\n"

#: ../main.c:35
msgid "         -s          differentiate source ports\n"
msgstr "         -s          diferenciar portas de origem\n"

#: ../main.c:36
msgid "         -t          show start times\n"
msgstr "         -t          mostrar horario inicial\n"

#: ../main.c:37
msgid "         -V          show version and copyright info\n"
msgstr "         -V          mostra informações sobre versão e copyright\n"

#: ../main.c:38
msgid "         -v          verbose, specify twice for more info\n"
msgstr ""
"         -v          detalhes, especifique duas vezes para mais informações\n"

#: ../main.c:39
msgid "         -y          differentiate TCP options\n"
msgstr "         -y          diferenciar opções de TCP\n"

#: ../main.c:40
msgid "         -z          show time interval\n"
msgstr "         -z          mostra intervalo de tempo\n"

#: ../main.c:43
msgid "Log summary mode (default):\n"
msgstr "Modo sumario de log (padrão):\n"

#: ../main.c:44
msgid ""
"         -b          show amount of data (sum of total packet lengths)\n"
msgstr ""
"         -b          mostra total de dados (soma dos tamanhos dos pacotes)\n"

#: ../main.c:45
msgid ""
"         -m <count>  only show entries with at least so many incidents\n"
msgstr ""
"         -m <num>    mostra somente entradas com um minimo de ocorrencias\n"

#: ../main.c:46
msgid "         -o <file>   specify output file\n"
msgstr "         -o <arq.>   especifica arquivo de saida\n"

#: ../main.c:47
msgid ""
"         -O <order>  define the sort order (see the man page for details)\n"
msgstr ""
"         -O <ordem>  define a ordem para ordenação (veja pagina man para "
"detalhes)\n"

#: ../main.c:48
msgid "         -w          HTML output\n"
msgstr "         -w          saída em HTML\n"

#: ../main.c:49
msgid "         -W          activate whois lookups for source addresses\n"
msgstr "         -W          activa verificação whois para endereços origem\n"

#: ../main.c:52
msgid "Interactive report mode:\n"
msgstr "Modo de relatório interativo:\n"

#: ../main.c:53
msgid "         -i <count>  interactive mode with report threshold\n"
msgstr "         -i <num>    modo interativo com limite minimo para ativação\n"

#: ../main.c:54
msgid "         -F <email>  report sender address\n"
msgstr "         -F <email>  email origem no envio do relatório\n"

#: ../main.c:55
#, c-format
msgid "                     (defaults to '%s')\n"
msgstr "                     (padrão : '%s')\n"

#: ../main.c:56
msgid ""
"         -T <email>  address of CERT or abuse contact to send report to\n"
msgstr ""
"         -T <email>  endereço do CERT ou contato de abuso para envio do "
"relatório\n"

#: ../main.c:57
msgid "         -C <email>  carbon copy recipients\n"
msgstr "         -C <email>  destinatarios para cópia\n"

#: ../main.c:58
msgid "         -I <file>   template file for report\n"
msgstr "         -I <arq.>   arquivo modelo para relatório\n"

#: ../main.c:59
#, c-format
msgid "                     (defaults to %s)\n"
msgstr "                     (padrão : %s)\n"

#: ../main.c:62
msgid "Realtime response mode:\n"
msgstr "Modo de resposta em tempo real:\n"

#: ../main.c:63
msgid ""
"         -R          realtime response as daemon (default action: log only)\n"
msgstr ""
"         -R          modo de resposta em tempo real (ação padrão: somente "
"registra)\n"

#: ../main.c:64
#, c-format
msgid "         -a <count>  alert threshold (defaults to %d entries)\n"
msgstr "         -a <num>  limite de ativação (padrão : %d entradas)\n"

#: ../main.c:65
#, c-format
msgid "         -l <time>   forget events this old (defaults to %d hours)\n"
msgstr ""
"         -l <tempo>  esquece eventos após periodo (padrão : %d horas)\n"

#: ../main.c:66
msgid ""
"         -k <IP/net> add this IP address or net to the list of known hosts\n"
msgstr "         -k <IP/rede> adiciona IP ou rede à lista de conhecidos\n"

#: ../main.c:67
msgid ""
"         -A          invoke notification script if threshold is reached\n"
msgstr "         -A          executa scripta de avisao ao chegar no limite\n"

#: ../main.c:68
msgid "         -B          invoke response action script (e.g. block host)\n"
msgstr ""
"         -B          executa script de resposta automatica (ex. bloqueia "
"maquina)\n"

#: ../main.c:69
msgid "         -X          activate internal status information web server\n"
msgstr "         -X          activa servidor web interno de informações\n"

#: ../main.c:95
msgid "You can contact the author at <Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."
msgstr ""
"Você pode contactar o autor via <Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."


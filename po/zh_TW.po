#: ../main.c:20
#, c-format
msgid "Usage: %s [options]\n"
msgstr "使用方法: %s [選項] \n"

#: ../main.c:21
msgid "Global options:\n"
msgstr "全局選項: \n"

#: ../main.c:22
#, c-format
msgid "         -c <file>   specify config file (defaults to %s)\n"
msgstr "        -c <文件>\t指定配置文件 (缺省 %s) \n"

#: ../main.c:23
msgid "         -D          do not differentiate destination IP addresses\n"
msgstr "        -D          \t不區分目標IP地址\n"

#: ../main.c:24
msgid "         -d          differentiate destination ports\n"
msgstr "        -d          \t區分目標端口\n"

#: ../main.c:25
#, fuzzy
msgid "         -e          show end times\n"
msgstr "        -t          \t顯示開始與結束時間\n"

#: ../main.c:26
#, c-format
msgid "         -f <file>   specify input file (defaults to %s)\n"
msgstr "        -f <文件>   \t指定輸入文件  (缺省 %s) \n"

#: ../main.c:27
msgid "         -h          this help\n"
msgstr "        -h          \t幫助文件\n"

#: ../main.c:28
msgid "         -L <file>   show time of first and last log entry in file\n"
msgstr "        -L <文件>   \t顯示文件中第一次與最後一次登陸紀錄的時間\n"

#: ../main.c:29
msgid "         -l <time>   process recent events only (defaults to off)\n"
msgstr "        -l <時間>   \t只處理當前事件 (缺省為關閉)\n"

#: ../main.c:30
#, fuzzy
msgid "         -N          resolve service names\n"
msgstr "        -n          \t解析域名\n"

#: ../main.c:31
msgid "         -n          resolve host names\n"
msgstr "        -n          \t解析域名\n"

#: ../main.c:32
msgid "         -P <format> use only parsers for specific formats\n"
msgstr "        -P <格式>  \n"

#: ../main.c:33
msgid "         -p          differentiate protocols\n"
msgstr "        -p          \t區分傳輸協議\n"

#: ../main.c:34
msgid "         -S          do not differentiate source IP addresses\n"
msgstr "        -S          \t不區分源IP地址\n"

#: ../main.c:35
msgid "         -s          differentiate source ports\n"
msgstr "        -s          \t區分源端口\n"

#: ../main.c:36
#, fuzzy
msgid "         -t          show start times\n"
msgstr "        -t          \t顯示開始與結束時間\n"

#: ../main.c:37
msgid "         -V          show version and copyright info\n"
msgstr "        -V          \t顯示版本與版權信息\n"

#: ../main.c:38
msgid "         -v          verbose, specify twice for more info\n"
msgstr "        -v          \t處理時顯示信息,使用 -vv 得到更多信息\n"

#: ../main.c:39
msgid "         -y          differentiate TCP options\n"
msgstr "        -y          \t區分TCP選項\n"

#: ../main.c:40
msgid "         -z          show time interval\n"
msgstr "        -z          \t顯示時間間隔\n"

#: ../main.c:43
msgid "Log summary mode (default):\n"
msgstr ""

#: ../main.c:44
msgid ""
"         -b          show amount of data (sum of total packet lengths)\n"
msgstr "        -b\t\t顯示數據量 (所有數據包長度的總和)\n"

#: ../main.c:45
msgid ""
"         -m <count>  only show entries with at least so many incidents\n"
msgstr "\t-m <數量> \t只顯示至少有這麼多數量的記錄\n"

#: ../main.c:46
msgid "         -o <file>   specify output file\n"
msgstr "        -o <文件>   \t指定輸出文件\n"

#: ../main.c:47
msgid ""
"         -O <order>  define the sort order (see the man page for details)\n"
msgstr "\t-O <排序方法> \t對輸出進行排序 (具體參見相應文檔\n"

#: ../main.c:48
msgid "         -w          HTML output\n"
msgstr "        -w          \t以HTML格式輸出\n"

#: ../main.c:49
msgid "         -W          activate whois lookups for source addresses\n"
msgstr "        -W          \t啟動對源IP地址的whois查詢\n"

#: ../main.c:52
msgid "Interactive report mode:\n"
msgstr "交互式報告模式:\n"

#: ../main.c:53
msgid "         -i <count>  interactive mode with report threshold\n"
msgstr "        -i <數量> \t報告流交互模式\n"

#: ../main.c:54
msgid "         -F <email>  report sender address\n"
msgstr "        -F <email>  \t發信人地址\n"

#: ../main.c:55
#, c-format
msgid "                     (defaults to '%s')\n"
msgstr "                   \t(缺省為 '%s')\n"

#: ../main.c:56
msgid ""
"         -T <email>  address of CERT or abuse contact to send report to\n"
msgstr "\t-T <email> \t收信人地址 (CERT或聯系人)\n"

#: ../main.c:57
msgid "         -C <email>  carbon copy recipients\n"
msgstr "        -C <email>  \t轉發地址\n"

#: ../main.c:58
msgid "         -I <file>   template file for report\n"
msgstr "        -I <文件>   \t報告模板文件\n"

#: ../main.c:59
#, c-format
msgid "                     (defaults to %s)\n"
msgstr "\t\t    \t(缺省為 %s)\n"

#: ../main.c:62
msgid "Realtime response mode:\n"
msgstr "實時回應模式:\n"

#: ../main.c:63
msgid ""
"         -R          realtime response as daemon (default action: log only)\n"
msgstr "\t-R \t\t實時回應作為傳輸代理程序 (缺省: 只有運行記錄)\n"

#: ../main.c:64
#, c-format
msgid "         -a <count>  alert threshold (defaults to %d entries)\n"
msgstr "\t-a <數量>   \t警告流 (缺省 %d 記錄)\n"

#: ../main.c:65
#, c-format
msgid "         -l <time>   forget events this old (defaults to %d hours)\n"
msgstr "\t-l <時間>   \t忽略此時間前舊事件 (缺省: %d小時)\n"

#: ../main.c:66
msgid ""
"         -k <IP/net> add this IP address or net to the list of known hosts\n"
msgstr "\t-k <IP/網絡> \t將此IP地址加入到已知主機列表中\n"

#: ../main.c:67
msgid ""
"         -A          invoke notification script if threshold is reached\n"
msgstr "\t-A\t\t如果有流進入啟動通知文本\n"

#: ../main.c:68
msgid "         -B          invoke response action script (e.g. block host)\n"
msgstr "\t-B\t    \t啟動回應文本 (比如 切斷主機與外部聯系)\n"

#: ../main.c:69
msgid "         -X          activate internal status information web server\n"
msgstr "\t-X          \t啟動內部狀態信息web服務器\n"

#: ../main.c:95
msgid "You can contact the author at <Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."
msgstr "請使用以下地址與作者聯系實際<Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."


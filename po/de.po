#: ../main.c:19
#, c-format
msgid "Usage: %s [options]\n"
msgstr "Gebrauch: %s [Optionen]\n"

#: ../main.c:20
msgid "Global options:\n"
msgstr "Globale Optionen:\n"

#: ../main.c:21
#, c-format
msgid "         -c <file>   specify config file (defaults to %s)\n"
msgstr "         -c <datei>  Konfigurationsdatei angeben (Vorgabe: %s)\n"

#: ../main.c:22
msgid "         -D          do not differentiate destination IP addresses\n"
msgstr "         -D          Ziel-IP-Adressen nicht unterscheiden\n"

#: ../main.c:23
msgid "         -d          differentiate destination ports\n"
msgstr "         -d          Zielports unterscheiden\n"

#: ../main.c:24
#, c-format
msgid "         -f <file>   specify input file (defaults to %s)\n"
msgstr "         -f <datei>  Eingabedatei angeben (Vorgabe: %s)\n"

#: ../main.c:25
msgid "         -h          this help\n"
msgstr "         -h          Diese Hilfe\n"

#: ../main.c:26
msgid "         -L <file>   show time of first and last log entry in file\n"
msgstr ""
"         -L <datei>  Zeitstempel des ersten und letzten Eintrags anzeigen\n"

#: ../main.c:27
msgid "         -l <time>   process recent events only (defaults to off)\n"
msgstr ""
"         -l <zeit>   Nur die jügsten Einträge bearbeiten (Vorgabe: "
"deaktiviert)\n"

#: ../main.c:28
msgid "         -n          resolve host names\n"
msgstr "         -n          Rechnernamen auflösen\n"

#: ../main.c:29
msgid "         -P <format> use only parsers for specific formats\n"
msgstr "         -P <format> Nur Parser für bestimmte Formate benutzen\n"

#: ../main.c:30
msgid "         -p          differentiate protocols\n"
msgstr "         -p          Protokolle unterscheiden\n"

#: ../main.c:31
msgid "         -S          do not differentiate source IP addresses\n"
msgstr "         -S          Quell-IP-Adressen nicht unterscheiden\n"

#: ../main.c:32
msgid "         -s          differentiate source ports\n"
msgstr "         -s          Quellports unterscheiden\n"

#: ../main.c:33
msgid "         -t          show start and end times\n"
msgstr "         -t          Anfangs- und Endzeiten anzeigen\n"

#: ../main.c:34
msgid "         -V          show version and copyright info\n"
msgstr "         -V          Versions- und Copyrightinformation anzeigen\n"

#: ../main.c:35
msgid "         -v          verbose, specify twice for more info\n"
msgstr ""
"         -v          Information bei der Verarbeitung anzeigen (-vv für "
"mehr)\n"

#: ../main.c:36
msgid "         -y          differentiate TCP options\n"
msgstr "         -y          TCP-Optionen unterscheiden\n"

#: ../main.c:37
msgid "         -z          show time interval\n"
msgstr "         -z          Zeitintervalle anzeigen\n"

#: ../main.c:40
msgid "Log summary mode (default):\n"
msgstr "Loganalysemodus (vorgabe):\n"

#: ../main.c:41
msgid ""
"         -b          show amount of data (sum of total packet lengths)\n"
msgstr "         -b          Datenmenge anzeigen (Summe der Paketlängen)\n"

#: ../main.c:42
msgid ""
"         -m <count>  only show entries with at least so many incidents\n"
msgstr ""
"         -m <anzahl> Nur Einträge mit mindestens dieser Anzahl anzeigen\n"

#: ../main.c:43
msgid "         -o <file>   specify output file\n"
msgstr "         -o <datei>  Ausgabedatei angeben\n"

#: ../main.c:44
msgid ""
"         -O <order>  define the sort order (see the man page for details)\n"
msgstr "         -O <ordnung> Ordnung der Ausgabe (siehe Dokumentation)\n"

#: ../main.c:45
msgid "         -w          HTML output\n"
msgstr "         -w          HTML-Ausgabe\n"

#: ../main.c:46
msgid "         -W          activate whois lookups for source addresses\n"
msgstr "         -W          whois-Nachfrage für Quell-IP-Adressen\n"

#: ../main.c:49
msgid "Interactive report mode:\n"
msgstr "Interaktiver Berichtmodus:\n"

#: ../main.c:50
msgid "         -i <count>  interactive mode with report threshold\n"
msgstr "         -i <anzahl> Interaktiver Modus mit Meldeschwelle\n"

#: ../main.c:51
msgid "         -F <email>  report sender address\n"
msgstr "         -F <email>  Absenderadresse des Berichts\n"

#: ../main.c:52
#, c-format
msgid "                     (defaults to '%s')\n"
msgstr "                     (Vorgabe: '%s')\n"

#: ../main.c:53
msgid ""
"         -T <email>  address of CERT or abuse contact to send report to\n"
msgstr ""
"         -T <email>  Adresse eines Empfängers (CERT oder Kontaktperson)\n"

#: ../main.c:54
msgid "         -C <email>  carbon copy recipients\n"
msgstr "         -C <email>  Empfängeradresse für Kopien\n"

#: ../main.c:55
msgid "         -I <file>   template file for report\n"
msgstr "         -I <datei>  Vorlage für den Bericht\n"

#: ../main.c:56
#, c-format
msgid "                     (defaults to %s)\n"
msgstr "                     (Vorgabe: %s)\n"

#: ../main.c:59
msgid "Realtime response mode:\n"
msgstr "Echtzeitantwortmodus:\n"

#: ../main.c:60
msgid ""
"         -R          realtime response as daemon (default action: log only)\n"
msgstr ""
"         -R          Echtzeitantwort als Daemon (Vorgegebene Aktion: "
"Loggen)\n"

#: ../main.c:61
#, c-format
msgid "         -a <count>  alert threshold (defaults to %d entries)\n"
msgstr "         -a <anzahl> Alarmschwelle (Vorgabe: %d Einträge)\n"

#: ../main.c:62
#, c-format
msgid "         -l <time>   forget events this old (defaults to %d hours)\n"
msgstr ""
"         -l <zeit>   Alte Ereignisse werden verworfen (Vorgabe: %d Stunden)\n"

#: ../main.c:63
msgid ""
"         -k <IP/net> add this IP address or net to the list of known hosts\n"
msgstr ""
"         -k <IP/Netz> Diese Adresse in der Liste der Bekannten aufnehmen\n"

#: ../main.c:64
msgid ""
"         -A          invoke notification script if threshold is reached\n"
msgstr ""
"         -A          Aufruf des Benachrichtigungsskripts bei err. der "
"Schwelle\n"

#: ../main.c:65
msgid "         -B          invoke response action script (e.g. block host)\n"
msgstr ""
"         -B          Aufruf des Antwortskipts (z.B. um Rechner zu sperren)\n"

#: ../main.c:66
msgid "         -X          activate internal status information web server\n"
msgstr ""
"         -X          Internen Webserver für Zustandsinformation aktivieren\n"

#: ../main.c:92
msgid "You can contact the author at <Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."
msgstr ""
"Der Autor ist unter <Boris.Wesslowski@RUS.Uni-Stuttgart.DE> zu erreichen."


#, c-format
msgid "               quick reference: -E[ie][hp][sd]<ip/number>\n"
msgstr "               Kurzübersicht: -E[ie][hp][sd]<ip/nummer>\n"

#: ../main.c:39
#, c-format
msgid "                                -E[ie][cb]<name>\n"
msgstr "                              -E[ie][cb]<name>\n"


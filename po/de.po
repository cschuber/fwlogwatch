#, c-format
msgid "  -F <email>   report sender address (defaults to '%s')\n"
msgstr "  -F <email>   Absenderadresse des Berichts (Vorgabe: %s)\n"

#: ../main.c:67
#, c-format
msgid "  -T <email>   address of CERT or abuse contact to send report to\n"
msgstr "  -T <email>   Adresse eines Empfängers (CERT oder Kontaktperson)\n"

#: ../main.c:68
#, c-format
msgid "  -I <file>    template file for report (defaults to %s)\n"
msgstr "  -I <datei>   Vorlage für den Bericht (Vorgabe: %s)\n"


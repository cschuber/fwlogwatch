#: ../main.c:20
#, c-format
msgid "Usage: %s [options]\n"
msgstr "Användningsexempel: %s [kommandoradsparametrar]\n"

#: ../main.c:21
msgid "Global options:\n"
msgstr "Globala parametrar:\n"

#: ../main.c:22
#, c-format
msgid "         -c <file>   specify config file (defaults to %s)\n"
msgstr ""
"         -c <filnamn>  Speficiera konfigurationsfil (Standardförval: %s)\n"

#: ../main.c:23
msgid "         -D          do not differentiate destination IP addresses\n"
msgstr "         -D          Differentiera ej mottangande IP adresser\n"

#: ../main.c:24
msgid "         -d          differentiate destination ports\n"
msgstr "         -d          Differentiera ej på mottagande portnummer\n"

#: ../main.c:25
#, fuzzy
msgid "         -e          show end times\n"
msgstr "         -t          Visa start- och stopptider\n"

#: ../main.c:26
#, c-format
msgid "         -f <file>   specify input file (defaults to %s)\n"
msgstr "         -f <filnamn>  Specificera indatafil (Standardförval: %s)\n"

#: ../main.c:27
msgid "         -h          this help\n"
msgstr "         -h          Denna hjälptext\n"

#: ../main.c:28
msgid "         -L <file>   show time of first and last log entry in file\n"
msgstr ""
"         -L <datei>  Visa tidsstämplar för första och sista logposten i "
"filen\n"

#: ../main.c:29
msgid "         -l <time>   process recent events only (defaults to off)\n"
msgstr ""
"         -l <zeit>   Bearbeta enbart nytillkomna händelser (Standardval: "
"avaktiverad)\n"

#: ../main.c:30
#, fuzzy
msgid "         -N          resolve service names\n"
msgstr "         -n          Utför DNS-namnuppslagning\n"

#: ../main.c:31
msgid "         -n          resolve host names\n"
msgstr "         -n          Utför DNS-namnuppslagning\n"

#: ../main.c:32
msgid "         -P <format> use only parsers for specific formats\n"
msgstr "         -P <format> använd enbart parser för specifika format\n"

#: ../main.c:33
msgid "         -p          differentiate protocols\n"
msgstr "         -p          Differentiera protokoll\n"

#: ../main.c:34
msgid "         -S          do not differentiate source IP addresses\n"
msgstr "         -S          Differentiera inte på avsändaradresser\n"

#: ../main.c:35
msgid "         -s          differentiate source ports\n"
msgstr "         -s          Differentiera på avsändarportar\n"

#: ../main.c:36
#, fuzzy
msgid "         -t          show start times\n"
msgstr "         -t          Visa start- och stopptider\n"

#: ../main.c:37
msgid "         -V          show version and copyright info\n"
msgstr "         -V          Visa versions- och upphovsrättsinformation\n"

#: ../main.c:38
msgid "         -v          verbose, specify twice for more info\n"
msgstr ""
"         -v          Ge utökad mängd information. Ge optionen dubbelt för "
"ännu högre grad av utdata (-vv)\n"

#: ../main.c:39
msgid "         -y          differentiate TCP options\n"
msgstr "         -y          Differentiera på TCP-Optioner\n"

#: ../main.c:40
msgid "         -z          show time interval\n"
msgstr "         -z          Visa tidsintervall\n"

#: ../main.c:43
msgid "Log summary mode (default):\n"
msgstr "Logganalysmod (standard):\n"

#: ../main.c:44
msgid ""
"         -b          show amount of data (sum of total packet lengths)\n"
msgstr ""
"         -b          Visa mängden data (Summering av totala paketstorlekar)\n"

#: ../main.c:45
msgid ""
"         -m <count>  only show entries with at least so many incidents\n"
msgstr ""
"         -m <antal> Visa enbart poster med åtminstone ANTAL incidenter\n"

#: ../main.c:46
msgid "         -o <file>   specify output file\n"
msgstr "         -o <fil>  Specificera utdatafil\n"

#: ../main.c:47
msgid ""
"         -O <order>  define the sort order (see the man page for details)\n"
msgstr ""
"         -O <ordning> Definiera sorteringsordning (Se manualblad för mer "
"beskrivning)\n"

#: ../main.c:48
msgid "         -w          HTML output\n"
msgstr "         -w          utdata som HTML\n"

#: ../main.c:49
msgid "         -W          activate whois lookups for source addresses\n"
msgstr "         -W          Utför en \"whois\"-fråga för avsändaradresser\n"

#: ../main.c:52
msgid "Interactive report mode:\n"
msgstr "Interaktiv avrapporteringsmod:\n"

#: ../main.c:53
msgid "         -i <count>  interactive mode with report threshold\n"
msgstr "         -i <antal> Interaktivt mod med tröskelvärde för rapport\n"

#: ../main.c:54
msgid "         -F <email>  report sender address\n"
msgstr "         -F <email>  Avsändaradressen för rapporten\n"

#: ../main.c:55
#, c-format
msgid "                     (defaults to '%s')\n"
msgstr "                     (Standardmässigt förval: '%s')\n"

#: ../main.c:56
msgid ""
"         -T <email>  address of CERT or abuse contact to send report to\n"
msgstr ""
"         -T <email>  E-postadress till CERT eller \"abuse\"-kontakt att "
"sända rapport till\n"

#: ../main.c:57
msgid "         -C <email>  carbon copy recipients\n"
msgstr "         -C <email>  Kännedomskopia av rapporten\n"

#: ../main.c:58
msgid "         -I <file>   template file for report\n"
msgstr "         -I <fil>  Mall för rapport\n"

#: ../main.c:59
#, c-format
msgid "                     (defaults to %s)\n"
msgstr "                     (Standardmässigt förval: %s)\n"

#: ../main.c:62
msgid "Realtime response mode:\n"
msgstr "Echtzeitantwortmodus:\n"

#: ../main.c:63
msgid ""
"         -R          realtime response as daemon (default action: log only)\n"
msgstr ""
"         -R          Realtidsåtgärder som bakgrundsprocess (daemon) "
"(Standardmässigt förval: enbart loggning)\n"

#: ../main.c:64
#, c-format
msgid "         -a <count>  alert threshold (defaults to %d entries)\n"
msgstr ""
"         -a <antal> Tröskelvärde för larm (Standardmässigt förval: %d "
"poster)\n"

#: ../main.c:65
#, c-format
msgid "         -l <time>   forget events this old (defaults to %d hours)\n"
msgstr ""
"         -l <tid>   Glöm händelser älder än (Standardmässigt förval: %d "
"timmar)\n"

#: ../main.c:66
msgid ""
"         -k <IP/net> add this IP address or net to the list of known hosts\n"
msgstr ""
"         -k <IP/Nät> Lägg till denna IP-adress eller detta nät till listan "
"över \"kända\" system\n"

#: ../main.c:67
msgid ""
"         -A          invoke notification script if threshold is reached\n"
msgstr ""
"         -A          Anropa notifieringsskript om tröskelvärdet nås "
"Schwelle\n"

#: ../main.c:68
msgid "         -B          invoke response action script (e.g. block host)\n"
msgstr ""
"         -B          Anropa åtgärdsskriptet (exempelvis för att spärra "
"adressen)\n"

#: ../main.c:69
msgid "         -X          activate internal status information web server\n"
msgstr ""
"         -X          Aktivera den interna statusinformationsfunktionen i "
"webbservern\n"

#: ../main.c:95
msgid "You can contact the author at <Boris.Wesslowski@RUS.Uni-Stuttgart.DE>."
msgstr ""
"Upphovsmannen till programmet kan nås på adressen <Boris.Wesslowski@RUS.Uni-"
"Stuttgart.DE>"

